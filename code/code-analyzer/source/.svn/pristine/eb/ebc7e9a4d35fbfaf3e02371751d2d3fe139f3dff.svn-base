package com.autostreets.biz.auction.model;

import java.math.BigDecimal;
import java.util.Date;

import com.autostreets.biz.auction.model.base.BaseVehicle;

/**
 * 会员车辆竞拍关系
 * 
 * @author meng.zhao
 */
public class MemberVehicle extends BaseVehicle {

    /**
	 * 
	 */
    private static final long serialVersionUID = 1L;

    /**
     * 竞拍价格
     */
    private BigDecimal price;

    /**
     * 竞拍人sid
     */
    private String biddingSid;

    /**
     * 竞得id
     */
    private Integer bidId;

    /**
     * 会员sid
     */
    private String bidderSid;

    /**
     * 拍卖时间
     */
    private Date planStartTime;

    /**
     * 拍卖状态
     */
    private Short status;

    /**
     * 车辆图片
     * 
     * @return
     */
    private String photoUrl;

    /**
     * 拍卖会编码
     */
    private Integer auctionId;
    /**
     * 拍品编码
     */
    private String avNo;

    private Date bidDate;

    private Integer avId;

    private BigDecimal startPrice;

    private Date planEndTime;

    /**
     * 关注ID
     * 
     * @return
     */
    private String favoriteSid;

    private Integer finalBidStatus;// 协商后，会员追加出价 0不可以追加， 1可以追加 2 已追加

    /**
     * 拍卖类型
     */
    private String auctionType;

    private BigDecimal finalBidPrice;// 协商后的加价

    public BigDecimal getFinalBidPrice() {
        return finalBidPrice;
    }

    public void setFinalBidPrice(BigDecimal finalBidPrice) {
        this.finalBidPrice = finalBidPrice;
    }

    public BigDecimal getPrice() {
        return price;
    }

    public void setPrice(BigDecimal price) {
        this.price = price;
    }

    public String getBiddingSid() {
        return biddingSid;
    }

    public void setBiddingSid(String biddingSid) {
        this.biddingSid = biddingSid;
    }

    public String getBidderSid() {
        return bidderSid;
    }

    public void setBidderSid(String bidderSid) {
        this.bidderSid = bidderSid;
    }

    public Date getPlanStartTime() {
        return planStartTime;
    }

    public void setPlanStartTime(Date planStartTime) {
        this.planStartTime = planStartTime;
    }

    public Short getStatus() {
        return status;
    }

    public void setStatus(Short status) {
        this.status = status;
    }

    public String getPhotoUrl() {
        return photoUrl;
    }

    public void setPhotoUrl(String photoUrl) {
        this.photoUrl = photoUrl;
    }

    public Integer getAuctionId() {
        return auctionId;
    }

    public void setAuctionId(Integer auctionId) {
        this.auctionId = auctionId;
    }

    public Integer getAvId() {
        return avId;
    }

    public void setAvId(Integer avId) {
        this.avId = avId;
    }

    public String getAvNo() {
        return avNo;
    }

    public void setAvNo(String avNo) {
        this.avNo = avNo;
    }

    public Date getBidDate() {
        return bidDate;
    }

    public void setBidDate(Date bidDate) {
        this.bidDate = bidDate;
    }

    public String getFavoriteSid() {
        return favoriteSid;
    }

    public void setFavoriteSid(String favoriteSid) {
        this.favoriteSid = favoriteSid;
    }

    public String getAuctionType() {
        return auctionType;
    }

    public void setAuctionType(String auctionType) {
        this.auctionType = auctionType;
    }

    public BigDecimal getStartPrice() {
        return startPrice;
    }

    public void setStartPrice(BigDecimal startPrice) {
        this.startPrice = startPrice;
    }

    public Date getPlanEndTime() {
        return planEndTime;
    }

    public void setPlanEndTime(Date planEndTime) {
        this.planEndTime = planEndTime;
    }

    public Integer getBidId() {
        return bidId;
    }

    public void setBidId(Integer bidId) {
        this.bidId = bidId;
    }

    /**
     * @return the finalBidStatus
     */
    public Integer getFinalBidStatus() {
        return finalBidStatus;
    }

    /**
     * @param finalBidStatus
     *            the finalBidStatus to set
     */
    public void setFinalBidStatus(Integer finalBidStatus) {
        this.finalBidStatus = finalBidStatus;
    }
}
